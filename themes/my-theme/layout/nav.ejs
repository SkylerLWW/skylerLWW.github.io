<ul id="menu">
    <!-- 导航栏 -->
    <% for ( menu_name in theme.menus ) { %>
        <li class="menu-item">
            <a href="<%- theme.menus[menu_name]['link'] %>" class="menu-item-link"><i
                        class="iconfont <%- theme.menus[menu_name]['icon'] %>"></i> <%- theme.menus[menu_name]['name']
                %></a>
        </li>
    <% } %>

    <!-- 搜索栏 -->
    <li id="search">
        <input class="search-input" type="text" placeholder="search">
        <span class="iconfont icon-chazhao"></span>
    </li>
</ul>

<!-- 音乐 -->
<div id="music" style="position: absolute; right: 0;">
    <button id="control_audio" onclick="controlAudio()"><i class="iconfont icon-music"></i></button>
    <audio id="audio" loop="loop" preload="auto">
        <source src="music/background_music.mp3" type="audio/mpeg">
    </audio>
</div>